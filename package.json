{
  "name": "@wikia/post-quecast",
  "version": "0.0.0-development",
  "description": "A Lightweight dependency injection container for typescript and javascript.",
  "contributors": [
    "Damian Bielecki <dbielecki@fandom.com>"
  ],
  "keywords": [
    "ioc",
    "di",
    "javascript",
    "typescript",
    "node",
    "dependency injection",
    "dependency inversion",
    "inversion of control"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Wikia/dependency-injection-js.git"
  },
  "license": "MIT",
  "files": [
    "dist/",
    "src/"
  ],
  "main": "./dist/commonjs/index.js",
  "module": "./dist/module/index.js",
  "typings": "./dist/types/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.js",
    "watch": "npm run build -- -w",
    "lint": "tslint ./src/**/*.ts ./e2e/**/*.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./e2e/**/*.ts -t verbose",
    "prettier": "prettier --write '**/*.{ts,js,json,css,scss,html}'",
    "format": "npm run prettier && npm run lint:fix",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^24.0.18",
    "@types/node": "^8.10.19",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^3.0.5",
    "istanbul": "^0.4.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.2",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.0",
    "rollup-plugin-uglify": "^6.0.2",
    "semantic-release": "^15.13.24",
    "source-map-support": "^0.5.9",
    "ts-jest": "^24.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.6.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
